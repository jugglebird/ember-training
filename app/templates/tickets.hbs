<article class="tickets">
  <ul>
    {{#each ticket in model}}
      {{#link-to 'ticket' ticket tagName="li"}}
        {{ticket.summary}}
      {{/link-to}}
    {{/each}}
  </ul>
</article>

<header class="ticket-header">
  {{#link-to "tickets.new" class="sui-bttn sui-bttn-primary new-ticket"}}
    New Ticket
  {{/link-to}}
</header>  

{{outlet}}
